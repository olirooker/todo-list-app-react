      localStorage.setItem('state', JSON.stringify(this.state));\n      }\n    );\n  };\n\n  toggleIsCompleted = (id) => {\n    this.setState(\n      (previousState) => {\n        const newToDos = previousState.todos.map((todo) => {\n          if (todo.item === id) {\n            const newTodo = {\n              item: todo.item,\n              isCompleted: !todo.isCompleted,\n            };\n            return newTodo;\n          } else return todo;\n        });\n        const newState = {\n          todos: newToDos,\n        };\n        return newState;\n      },\n      () => {\n        localStorage.setItem('state', JSON.stringify(this.state));\n      }\n    );\n  };\n\n  changeFilter = (condition) => {\n    this.setState({ filterBy: condition });\n  };\n\n  filterList = () => {\n    return this.state.todos.filter((todo) => {\n      if (this.state.filterBy === 'all') return todo;\n      else if (this.state.filterBy === 'active')\n        return todo.isCompleted === false;\n      else if (this.state.filterBy === 'completed')\n        return todo.isCompleted === true;\n    });\n  };\n\n  render() {\n    return (\n      <div className='app-container'>\n        <Header />\n        <TaskAdder addToDo={this.addToDo} />\n        <Filter\n          changeFilter={this.changeFilter}\n          filterBy={this.state.filterBy}\n        />\n        <List\n          todos={this.filterList(this.state.todos)}\n          deleteToDo={this.deleteToDo}\n          toggleIsCompleted={this.toggleIsCompleted}\n        />\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/olirooker/Documents/Northcoders/Frontend/todo-list-app-react/src/components/List.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/todo-list-app-react/src/components/TaskAdder.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/todo-list-app-react/src/components/Header.js",[],"/Users/olirooker/Documents/Northcoders/Frontend/todo-list-app-react/src/index.js",[],"/Users/olirooker/Documents/Northcoders/